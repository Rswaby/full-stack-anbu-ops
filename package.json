{
  "name": "full-stack-course-manager",
  "version": "1.0.0",
  "description": "run scripts from root directory",
  "main": "index.js",
  "scripts": {
    "client_install": "cd client && npm install",
    "server_install": "cd rest_api && npm install",
    "component_install": "concurrently \"npm run server_install\" \"npm run client_install\"",
    "client": "cd client && npm start",
    "server": "cd rest_api && npm start",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rswaby/full-stack-course-manager.git"
  },
  "keywords": [
    "node.js",
    "express.js",
    "javascript",
    "react.js"
  ],
  "author": "Rohan Swaby",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rswaby/full-stack-course-manager/issues"
  },
  "homepage": "https://github.com/Rswaby/full-stack-course-manager#readme",
  "dependencies": {
    "concurrently": "^6.3.0"
  }
}
